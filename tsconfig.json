{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "components/*": ["components/*"],
      "layout/*": ["layout/*"],
      "pages/*": ["pages/*"],
      "store/*": ["store/*"],
      "types/*": ["types/*"],
      "lib/*": ["lib/*"],
      "constants/*": ["lib/constants/*"],
      "models/*": ["lib/models/*"],
      "http/*": ["lib/http/*"],
      "styles/*": ["styles/*"],
      "UI/*": ["UI/*"],
      "icon/*": ["assets/Icon/*"],
      "image/*": ["assets/Image/*"],
      "hooks/*": ["hooks/*"],
      "fetch/*": ["lib/fetch/*"]
    }
  },
  "include": [
    "src"
  ]
}
